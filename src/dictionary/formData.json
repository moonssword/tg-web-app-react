{
    "house_type": {
      "label": "Тип жилья",
      "type": "radio",
      "required": true,
      "options": [
        { "value": "house", "label": "Дом" },
        { "value": "apartment", "label": "Квартира" },
        { "value": "room", "label": "Комната" }
      ]
    },
    "ad_type": {
      "label": "Форма размещения",
      "type": "radio",
      "depends_on": ["house_type"], 
      "visible_if": {
        "house_type": ["apartment", "room", "house"]
      },
      "required": true,
      "options": [
        { "value": "rentIn", "label": "Сниму" },
        { "value": "rentOut", "label": "Сдам" }
      ]
    },
    "location": {
      "label": "Местоположение",
      "type": "custom",
      "depends_on": ["house_type", "ad_type"], 
      "visible_if": {
        "house_type" : ["apartment", "room", "house"],
        "ad_type": ["rentIn", "rentOut"]
      },
      "required": true
    },
    "address": {
      "label": "Улица, дом",
      "type": "text",
      "depends_on": ["ad_type"], 
      "visible_if": {
        "ad_type": ["rentOut"]
      },
      "required": true,
      "placeholder": "Введите улицу и № дома"
    },
    "duration": {
      "label": "Срок аренды ",
      "type": "radio",
      "depends_on": ["ad_type"], 
      "visible_if": {
        "ad_type": ["rentOut", "rentIn"]
      },
      "required": true,
      "options": [
        { "value": "daily_rent", "label": "Посуточно" },
        { "value": "long_time", "label": "На длительный срок" }        
      ]
    },
    "rooms": {
      "label": "Количество комнат",
      "type": "radio",
      "depends_on": ["house_type", "ad_type"], 
      "visible_if": {
        "house_type": ["apartment"],
        "ad_type": ["rentIn", "rentOut"]
      },
      "required": true,
      "options": [
        { "value": 1, "label": "1" },
        { "value": 2, "label": "2" },
        { "value": 3, "label": "3" },
        { "value": 4, "label": "4" },
        { "value": 5, "label": "5+" }
      ]
    },
    "room_type": {
      "label": "Тип размещения",
      "type": "radio",
      "depends_on": ["house_type", "ad_type"], 
      "visible_if": {
        "house_type": ["room"],
        "ad_type": ["rentOut"]
      },
      "required": true,
      "options": [
        { "value": "room", "label": "Комната" },
        { "value": "bed_space", "label": "Койко-место" }
      ]
    },
    "room_location": {
      "label": "Расположение комнаты",
      "type": "radio",
      "depends_on": ["house_type", "ad_type"], 
      "visible_if": {
        "house_type": ["room"],
        "ad_type": ["rentOut"]
      },
      "required": true,
      "options": [
        { "value": "apartment", "label": "Квартира" },
        { "value": "hostel", "label": "Хостел" },
        { "value": "hotel", "label": "Гостиница" }
      ]
    },
    "bed_capacity": {
      "label": "Количество спальных мест",
      "type": "radio",
      "depends_on": ["house_type", "ad_type"], 
      "visible_if": {
        "ad_type": ["rentOut"],
        "house_type": ["room"]
      },
      "required": false,
      "options": [
        { "value": 1, "label": "1" },
        { "value": 2, "label": "2" },
        { "value": 3, "label": "3" },
        { "value": 4, "label": "4" },
        { "value": 5, "label": "5+" }
      ]
    },
    "area": {
      "label": "Площадь (м²)",
      "type": "number",
      "depends_on": ["house_type", "ad_type"],
      "visible_if": {
        "house_type": ["apartment", "room", "house"],
        "ad_type": ["rentOut"]
      },
      "required": true,
      "placeholder": "Введите площадь квартиры"
    },
    "price": {
      "label": "Цена",
      "type": "number",
      "depends_on": ["house_type", "ad_type"], 
      "visible_if": {
        "house_type": ["apartment", "room", "house"],
        "ad_type": ["rentOut"]
      },
      "required": true,
      "placeholder": "Введите цену"
    },
    "price_min": {
      "label": "Цена от",
      "type": "number",
      "depends_on": ["house_type", "ad_type"], 
      "visible_if": {
        "house_type": ["apartment", "room", "house"],
        "ad_type": ["rentIn"]
      },
      "required": true,
      "placeholder": "От"
    },
    "price_max": {
      "label": "Цена до",
      "type": "number",
      "depends_on": ["house_type", "ad_type"], 
      "visible_if": {
        "house_type": ["apartment", "room", "house"],
        "ad_type": ["rentIn"]
      },
      "required": true,
      "placeholder": "До"
    },
    "currency": {
      "label": "Валюта",
      "type": "radio",
      "depends_on": ["house_type", "ad_type"], 
      "visible_if": {
        "house_type": ["apartment", "room", "house"],
        "ad_type": ["rentOut"]
      },
      "options": [
        { "value": "KZT", "label": "KZT" },
        { "value": "USD", "label": "USD" },
        { "value": "EUR", "label": "EUR" }
      ],
      "defaultValue": "KZT"
    },
    "deposit": {
      "label": "Депозит",
      "type": "radio",
      "depends_on": ["ad_type", "duration"], 
      "visible_if": {
        "ad_type": ["rentOut"],
        "long_time": ["long_time"]
      },
      "options": [
        { "value": true, "label": "Да" },
        { "value": false, "label": "Нет" }
      ],
      "required": true
    },
    "deposit_value": {
      "label": "Размер депозита (%)",
      "type": "radio",
      "depends_on": ["deposit"], 
      "visible_if": {
        "deposit": [true]
    },
      "options": [
        { "value": "10", "label": "10" },
        { "value": "20", "label": "20" },
        { "value": "30", "label": "30" },
        { "value": "40", "label": "40" },
        { "value": "50", "label": "50" }
      ],
      "required": true
    },
    "floor_current": {
      "label": "Этаж",
      "type": "number",
      "depends_on": ["house_type", "ad_type"], 
      "visible_if": {
        "house_type": ["apartment", "room", "house"],
        "ad_type": ["rentOut"]
      },
      "required": true,
      "placeholder": "Введите этаж"
    },
    "floor_total": {
      "label": "Этажей всего",
      "type": "number",
      "depends_on": ["house_type", "ad_type"], 
      "visible_if": {
        "house_type": ["apartment", "room", "house"],
        "ad_type": ["rentOut"]
      },
      "required": true,
      "placeholder": "Введите количество этажей"
    },
    "phone": {
      "label": "Телефон",
      "type": "number",
      "depends_on": ["house_type"], 
      "visible_if": {
        "house_type": ["apartment", "room", "house"]
      },
      "required": true,
      "placeholder": "+7 777 777 77 77",
      "pattern": "\\+7\\s?\\d{3}\\s?\\d{3}\\s?\\d{2}\\s?\\d{2}"
    },
    "author": {
      "label": "Размещает объявление",
      "type": "radio",
      "depends_on": ["house_type", "ad_type"], 
      "visible_if": {
        "house_type": ["apartment", "room", "house"],
        "ad_type": ["rentOut"]
      },
      "required": true,
      "options": [
        { "value": "owner", "label": "Собственник" },
        { "value": "agent", "label": "Посредник" }
      ]
    },
    "communication_method": {
      "label": "Способ связи",
      "type": "checkbox",
      "depends_on": ["house_type", "ad_type"], 
      "visible_if": {
        "house_type": ["apartment", "room", "house"],
        "ad_type": ["rentOut"]
      },
      "required": true,
      "options": [
        { "value": "call", "label": "Звонки" },
        { "value": "telegram", "label": "Telegram" },
        { "value": "whatsapp", "label": "WhatsApp" }
      ]
    },
    "tg_username": {
      "label": "Telegram",
      "type": "text",
      "depends_on": ["telegram"],
      "visible_if": {"telegram": [true]},
      "required": true,
      "placeholder": ""
    },
    "description": {
      "label": "Описание",
      "type": "textarea",
      "depends_on": ["ad_type"],
      "visible_if": {
        "ad_type": ["rentOut"]
      },
      "required": true,
      "placeholder": "Введите описание (не более 400 символов)"
    },
    "furniture": {
      "label": "Мебель",
      "type": "checkbox",
      "depends_on": ["ad_type"],
      "visible_if": {
        "ad_type": ["rentOut"]
      },
      "options": [
        { "value": "kitchen", "label": "Кухня" },
        { "value": "wardrobe", "label": "Хранение одежды" },
        { "value": "sleeping_places", "label": "Спальные места" }
      ]
    },
    "toilet": {
      "label": "Санузел",
      "type": "checkbox",
      "depends_on": ["ad_type"],
      "visible_if": {
        "ad_type": ["rentOut"]
      },
      "options": [
        { "value": "combined_toilet", "label": "Совмещённый" },
        { "value": "separate_toilet", "label": "Раздельный" }
      ]
    },
    "bathroom": {
      "label": "Ванная комната",
      "type": "checkbox",
      "depends_on": ["ad_type"],
      "visible_if": {
        "ad_type": ["rentOut"]
      },
      "options": [
        { "value": "bath", "label": "Ванная" },
        { "value": "shower", "label": "Душевая кабина" }
      ]
    },
    "technic": {
      "label": "Техника",
      "depends_on": ["ad_type"],
      "visible_if": {
        "ad_type": ["rentOut"]
      },
      "type": "checkbox",
      "options": [
        { "value": "air_conditioner", "label": "Кондиционер" },
        { "value": "fridge", "label": "Холодильник" },
        { "value": "stove", "label": "Плита" },
        { "value": "microwave", "label": "Микроволновка" },
        { "value": "washing_machine", "label": "Стиральная машина" },
        { "value": "dishwasher", "label": "Посудомоечная машина" },
        { "value": "tv", "label": "Телевизор" },
        { "value": "iron", "label": "Утюг" },
        { "value": "dryer", "label": "Сушилка" },
        { "value": "vacuum_cleaner", "label": "Пылесос" }
      ]
    },
    "internet_and_tv": {
      "label": "Интернет и ТВ",
      "type": "checkbox",
      "depends_on": ["ad_type"],
      "visible_if": {
        "ad_type": ["rentOut"]
      },
      "options": [
        { "value": "wifi", "label": "Wi-Fi" },
        { "value": "television", "label": "Телевидение" }
      ]
    },
    "comfort": {
      "label": "Комфорт",
      "type": "checkbox",
      "depends_on": ["ad_type", "duration"],
      "visible_if": {
        "ad_type": ["rentOut"],
        "duration": ["daily_rent"]
      },
      "options": [
        { "value": "bed_linen", "label": "Постельное белье" },
        { "value": "towels", "label": "Полотенца" },
        { "value": "hygiene_items", "label": "Средства гигиены" }
      ]
    },
    "condition": {
      "label": "Ремонт:",
      "type": "radio",
      "depends_on": ["ad_type"],
      "visible_if": {
        "ad_type": []
      },
      "required": true,
      "options": [
        { "value": "needs_renovation", "label": "Требуется" },
        { "value": "cosmetic", "label": "Косметический" },
        { "value": "euro", "label": "Евро" },
        { "value": "designer", "label": "Дизайнерский" }
      ]
    },
    "rental_options": {
      "label": "Правила заселения",
      "type": "checkbox",
      "depends_on": ["ad_type"],
      "visible_if": {
        "ad_type": ["rentOut"]
      },
      "required": true,
      "options": [
        { "value": "family", "label": "Семейной паре" },
        { "value": "single", "label": "Одному человеку" },
        { "value": "with_child", "label": "Можно с детьми" },
        { "value": "with_pets", "label": "Можно с животными" }
      ]
    },
    "max_guests": {
      "label": "Максимум гостей",
      "type": "radio",
      "depends_on": ["ad_type"],
      "visible_if": {
        "ad_type": ["rentOut"]
      },
      "required": false,
      "options": [
        { "value": 1, "label": "1" },
        { "value": 2, "label": "2" },
        { "value": 3, "label": "3" },
        { "value": 4, "label": "4" },
        { "value": 5, "label": "5+" }
      ]
    }
  }
  